{
  "python.defaultInterpreterPath": "/isaac-sim/python.sh",
  "python.analysis.extraPaths": [
    "/isaac-sim/kit/kernel/py",
    "/isaac-sim/kit/exts",
    "/isaac-sim/kit/extscore",
    "/isaac-sim/exts",
    "/isaac-sim/extscache",
    "/isaac-sim/extsDeprecated",
    "/isaac-sim/extsUser",
    "/isaac-sim/kit/python/lib/python3.11/site-packages",
    "/isaac-sim/kit/python/lib/python3.11/site-packages/isaaclab/source/isaaclab",
    "${workspaceFolder}/costnav_isaaclab",
